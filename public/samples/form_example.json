{
  "type": "View",
  "orientation": "vertical",
  "padding": [16, 16],
  "child": [
    {
      "type": "Label",
      "text": "Sign Up",
      "fontSize": 24,
      "fontWeight": "bold"
    },
    {
      "type": "TextField",
      "id": "email",
      "hint": "Email address"
    },
    {
      "type": "TextField",
      "id": "password",
      "hint": "Password",
      "secureTextEntry": true
    },
    {
      "type": "Button",
      "text": "Create Account",
      "onClick": "handleSignUp"
    }
  ]
}
