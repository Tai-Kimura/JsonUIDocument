{
  "type": "View",
  "bindingGroup": "userProfile",
  "child": [
    {
      "type": "Label",
      "text": "@{viewModel.user.name}"
    },
    {
      "type": "Label",
      "text": "@{viewModel.user.email}"
    },
    {
      "type": "Image",
      "src": "@{viewModel.user.avatarUrl}"
    }
  ]
}
