{
  "type": "View",
  "id": "components_page",
  "width": "matchParent",
  "orientation": "vertical",
  "child": [
    {},
    {
      "type": "View",
      "id": "components_hero",
      "width": "matchParent",
      "padding": [
        48,
        24
      ],
      "orientation": "vertical",
      "gravity": "center",
      "background": "#F6F7F9",
      "child": [
        {
          "type": "Label",
          "text": "components_title",
          "fontSize": 48,
          "fontWeight": "bold",
          "fontColor": "#23272F",
          "textAlign": "Center"
        },
        {
          "style": "spacer_16"
        },
        {
          "type": "Label",
          "text": "components_subtitle",
          "fontSize": 20,
          "fontColor": "#5E687E",
          "textAlign": "Center"
        }
      ]
    },
    {
      "type": "View",
      "id": "components_content",
      "width": "matchParent",
      "padding": [
        48,
        24
      ],
      "orientation": "vertical",
      "background": "#FFFFFF",
      "child": [
        {
          "include": "components/component_section_layout",
          "data": {
            "viewModel": "@{viewModel}"
          }
        },
        {
          "style": "spacer_48"
        },
        {
          "include": "components/component_section_text",
          "data": {
            "viewModel": "@{viewModel}"
          }
        },
        {
          "style": "spacer_48"
        },
        {
          "include": "components/component_section_input",
          "data": {
            "viewModel": "@{viewModel}"
          }
        },
        {
          "style": "spacer_48"
        },
        {
          "type": "View",
          "orientation": "horizontal",
          "gravity": "center",
          "child": [
            {
              "type": "Button",
              "style": "button_outline",
              "text": "components_prev",
              "onClick": "@{onClickQuickStart}"
            },
            {
              "type": "View",
              "width": 16
            },
            {
              "type": "Button",
              "style": "button_primary",
              "text": "components_next",
              "onClick": "@{onClickStyling}"
            }
          ]
        }
      ]
    }
  ]
}