{
  "type": "View",
  "id": "ref_components_page",
  "width": "matchParent",
  "height": "matchParent",
  "orientation": "horizontal",
  "child": [
    {
      "data": [
        { "class": "RefComponentsViewModel", "name": "viewModel" }
      ]
    },
    { "type": "ReferenceSidebar" },
    {
      "type": "ScrollView",
      "id": "ref_components_scroll",
      "width": "matchParent",
      "height": "matchParent",
      "child": [
        {
          "type": "View",
          "id": "ref_components_content",
          "width": "matchParent",
          "padding": [48, 24],
          "orientation": "vertical",
          "background": "#FFFFFF",
          "child": [
            {
              "type": "Label",
              "text": "ref_components",
              "fontSize": 32,
              "fontWeight": "bold",
              "fontColor": "#23272F",
              "bottomMargin": 8
            },
            {
              "type": "Label",
              "text": "ref_components_overview",
              "fontSize": 16,
              "fontColor": "#4B5563",
              "bottomMargin": 32,
              "lineHeight": 1.6
            },
            {
              "type": "Label",
              "text": "ref_layout_components",
              "fontSize": 20,
              "fontWeight": "bold",
              "fontColor": "#23272F",
              "bottomMargin": 12
            },
            {
              "type": "View",
              "orientation": "vertical",
              "width": "matchParent",
              "bottomMargin": 24,
              "child": [
                {
                  "type": "View",
                  "orientation": "horizontal",
                  "width": "matchParent",
                  "padding": [16, 12],
                  "background": "#F9FAFB",
                  "bottomMargin": 8,
                  "cornerRadius": 8,
                  "canTap": true,
                  "tapBackground": "#E5E7EB",
                  "onClick": "@{viewModel.goToView}",
                  "child": [
                    { "type": "Label", "text": "comp_view_title", "fontSize": 14, "fontWeight": "semibold", "fontColor": "#087EA4", "width": 120 },
                    { "type": "Label", "text": "components_view_desc", "fontSize": 14, "fontColor": "#4B5563" }
                  ]
                },
                {
                  "type": "View",
                  "orientation": "horizontal",
                  "width": "matchParent",
                  "padding": [16, 12],
                  "background": "#F9FAFB",
                  "bottomMargin": 8,
                  "cornerRadius": 8,
                  "canTap": true,
                  "tapBackground": "#E5E7EB",
                  "onClick": "@{viewModel.goToScrollView}",
                  "child": [
                    { "type": "Label", "text": "comp_scrollview_title", "fontSize": 14, "fontWeight": "semibold", "fontColor": "#087EA4", "width": 120 },
                    { "type": "Label", "text": "components_scroll_desc", "fontSize": 14, "fontColor": "#4B5563" }
                  ]
                },
                {
                  "type": "View",
                  "orientation": "horizontal",
                  "width": "matchParent",
                  "padding": [16, 12],
                  "background": "#F9FAFB",
                  "cornerRadius": 8,
                  "canTap": true,
                  "tapBackground": "#E5E7EB",
                  "onClick": "@{viewModel.goToCollection}",
                  "child": [
                    { "type": "Label", "text": "comp_collection_title", "fontSize": 14, "fontWeight": "semibold", "fontColor": "#087EA4", "width": 120 },
                    { "type": "Label", "text": "components_collection_desc", "fontSize": 14, "fontColor": "#4B5563" }
                  ]
                }
              ]
            },
            {
              "type": "Label",
              "text": "ref_text_components",
              "fontSize": 20,
              "fontWeight": "bold",
              "fontColor": "#23272F",
              "bottomMargin": 12
            },
            {
              "type": "View",
              "orientation": "vertical",
              "width": "matchParent",
              "bottomMargin": 24,
              "child": [
                {
                  "type": "View",
                  "orientation": "horizontal",
                  "width": "matchParent",
                  "padding": [16, 12],
                  "background": "#F9FAFB",
                  "bottomMargin": 8,
                  "cornerRadius": 8,
                  "canTap": true,
                  "tapBackground": "#E5E7EB",
                  "onClick": "@{viewModel.goToLabel}",
                  "child": [
                    { "type": "Label", "text": "comp_label_title", "fontSize": 14, "fontWeight": "semibold", "fontColor": "#087EA4", "width": 120 },
                    { "type": "Label", "text": "components_label_desc", "fontSize": 14, "fontColor": "#4B5563" }
                  ]
                },
                {
                  "type": "View",
                  "orientation": "horizontal",
                  "width": "matchParent",
                  "padding": [16, 12],
                  "background": "#F9FAFB",
                  "bottomMargin": 8,
                  "cornerRadius": 8,
                  "canTap": true,
                  "tapBackground": "#E5E7EB",
                  "onClick": "@{viewModel.goToTextField}",
                  "child": [
                    { "type": "Label", "text": "comp_textfield_title", "fontSize": 14, "fontWeight": "semibold", "fontColor": "#087EA4", "width": 120 },
                    { "type": "Label", "text": "components_textfield_desc", "fontSize": 14, "fontColor": "#4B5563" }
                  ]
                },
                {
                  "type": "View",
                  "orientation": "horizontal",
                  "width": "matchParent",
                  "padding": [16, 12],
                  "background": "#F9FAFB",
                  "cornerRadius": 8,
                  "canTap": true,
                  "tapBackground": "#E5E7EB",
                  "onClick": "@{viewModel.goToTextView}",
                  "child": [
                    { "type": "Label", "text": "comp_textview_title", "fontSize": 14, "fontWeight": "semibold", "fontColor": "#087EA4", "width": 120 },
                    { "type": "Label", "text": "components_textview_desc", "fontSize": 14, "fontColor": "#4B5563" }
                  ]
                }
              ]
            },
            {
              "type": "Label",
              "text": "ref_input_components",
              "fontSize": 20,
              "fontWeight": "bold",
              "fontColor": "#23272F",
              "bottomMargin": 12
            },
            {
              "type": "View",
              "orientation": "vertical",
              "width": "matchParent",
              "child": [
                {
                  "type": "View",
                  "orientation": "horizontal",
                  "width": "matchParent",
                  "padding": [16, 12],
                  "background": "#F9FAFB",
                  "bottomMargin": 8,
                  "cornerRadius": 8,
                  "canTap": true,
                  "tapBackground": "#E5E7EB",
                  "onClick": "@{viewModel.goToButton}",
                  "child": [
                    { "type": "Label", "text": "comp_button_title", "fontSize": 14, "fontWeight": "semibold", "fontColor": "#087EA4", "width": 120 },
                    { "type": "Label", "text": "components_button_desc", "fontSize": 14, "fontColor": "#4B5563" }
                  ]
                },
                {
                  "type": "View",
                  "orientation": "horizontal",
                  "width": "matchParent",
                  "padding": [16, 12],
                  "background": "#F9FAFB",
                  "bottomMargin": 8,
                  "cornerRadius": 8,
                  "canTap": true,
                  "tapBackground": "#E5E7EB",
                  "onClick": "@{viewModel.goToSwitch}",
                  "child": [
                    { "type": "Label", "text": "comp_switch_title", "fontSize": 14, "fontWeight": "semibold", "fontColor": "#087EA4", "width": 120 },
                    { "type": "Label", "text": "components_switch_desc", "fontSize": 14, "fontColor": "#4B5563" }
                  ]
                },
                {
                  "type": "View",
                  "orientation": "horizontal",
                  "width": "matchParent",
                  "padding": [16, 12],
                  "background": "#F9FAFB",
                  "cornerRadius": 8,
                  "canTap": true,
                  "tapBackground": "#E5E7EB",
                  "onClick": "@{viewModel.goToSlider}",
                  "child": [
                    { "type": "Label", "text": "comp_slider_title", "fontSize": 14, "fontWeight": "semibold", "fontColor": "#087EA4", "width": 120 },
                    { "type": "Label", "text": "components_slider_desc", "fontSize": 14, "fontColor": "#4B5563" }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}
