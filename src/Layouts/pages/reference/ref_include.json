{
  "type": "View",
  "id": "ref_include_page",
  "width": "matchParent",
  "height": "matchParent",
  "orientation": "horizontal",
  "child": [
    {
      "data": [
        { "class": "RefIncludeViewModel", "name": "viewModel" }
      ]
    },
    { "type": "ReferenceSidebar" },
    {
      "type": "ScrollView",
      "id": "ref_include_scroll",
      "width": "matchParent",
      "height": "matchParent",
      "child": [
        {
          "type": "View",
          "id": "ref_include_content",
          "width": "matchParent",
          "padding": [48, 24],
          "orientation": "vertical",
          "background": "#FFFFFF",
          "child": [
            {
              "type": "Label",
              "text": "ref_include",
              "fontSize": 32,
              "fontWeight": "bold",
              "fontColor": "#23272F",
              "bottomMargin": 8
            },
            {
              "type": "Label",
              "text": "ref_include_overview",
              "fontSize": 16,
              "fontColor": "#4B5563",
              "bottomMargin": 32,
              "lineHeight": 1.6
            },
            {
              "type": "Label",
              "text": "ref_include_syntax",
              "fontSize": 20,
              "fontWeight": "bold",
              "fontColor": "#23272F",
              "bottomMargin": 12
            },
            {
              "type": "View",
              "width": "matchParent",
              "padding": [16, 12],
              "background": "#F6F8FA",
              "cornerRadius": 8,
              "bottomMargin": 24,
              "child": [
                {
                  "type": "Label",
                  "text": "{ \"include\": \"path/to/component.json\" }",
                  "fontSize": 14,
                  "fontColor": "#24292E"
                }
              ]
            },
            {
              "type": "Label",
              "text": "ref_include_attributes_title",
              "fontSize": 20,
              "fontWeight": "bold",
              "fontColor": "#23272F",
              "bottomMargin": 16
            },
            {
              "type": "Collection",
              "width": "matchParent",
              "borderWidth": 1,
              "borderColor": "#E5E7EB",
              "cornerRadius": 8,
              "clipToBounds": true,
              "bottomMargin": 32,
              "headerClasses": ["components/include_attribute_table_header"],
              "cellClasses": ["components/include_attribute_row"],
              "items": "@{viewModel.includeAttributes}"
            },
            {
              "type": "Label",
              "text": "ref_include_example",
              "fontSize": 20,
              "fontWeight": "bold",
              "fontColor": "#23272F",
              "bottomMargin": 12
            },
            {
              "type": "CodeBlock",
              "file": "include_basic.json",
              "language": "json",
              "showPreview": false,
              "bottomMargin": 32
            },
            {
              "type": "Label",
              "text": "ref_include_data_sharing_title",
              "fontSize": 20,
              "fontWeight": "bold",
              "fontColor": "#23272F",
              "bottomMargin": 12
            },
            {
              "type": "Label",
              "text": "ref_include_data_sharing_desc",
              "fontSize": 16,
              "fontColor": "#4B5563",
              "bottomMargin": 16,
              "lineHeight": 1.6
            },
            {
              "type": "Label",
              "text": "ref_include_data_sharing_example_title",
              "fontSize": 16,
              "fontWeight": "600",
              "fontColor": "#23272F",
              "bottomMargin": 12
            },
            {
              "type": "CodeBlock",
              "file": "include_shared_data.json",
              "language": "json",
              "showPreview": false,
              "bottomMargin": 32
            },
            {
              "type": "Label",
              "text": "ref_include_binding_id_title",
              "fontSize": 20,
              "fontWeight": "bold",
              "fontColor": "#23272F",
              "bottomMargin": 12
            },
            {
              "type": "Label",
              "text": "ref_include_binding_id_desc",
              "fontSize": 16,
              "fontColor": "#4B5563",
              "bottomMargin": 16,
              "lineHeight": 1.6
            },
            {
              "type": "CodeBlock",
              "file": "include_binding_id.json",
              "language": "json",
              "showPreview": false,
              "bottomMargin": 32
            },
            {
              "type": "Label",
              "text": "ref_include_nested_title",
              "fontSize": 20,
              "fontWeight": "bold",
              "fontColor": "#23272F",
              "bottomMargin": 12
            },
            {
              "type": "Label",
              "text": "ref_include_nested_desc",
              "fontSize": 16,
              "fontColor": "#4B5563",
              "bottomMargin": 16,
              "lineHeight": 1.6
            },
            {
              "type": "CodeBlock",
              "file": "include_nested.json",
              "language": "json",
              "showPreview": false,
              "bottomMargin": 32
            },
            {
              "type": "Label",
              "text": "ref_include_best_practices_title",
              "fontSize": 20,
              "fontWeight": "bold",
              "fontColor": "#23272F",
              "bottomMargin": 16
            },
            {
              "type": "View",
              "width": "matchParent",
              "padding": [20, 16],
              "background": "#F0FDF4",
              "cornerRadius": 8,
              "bottomMargin": 32,
              "child": [
                {
                  "type": "Label",
                  "text": "• ref_include_practice_1",
                  "fontSize": 14,
                  "fontColor": "#166534",
                  "bottomMargin": 8,
                  "lineHeight": 1.5
                },
                {
                  "type": "Label",
                  "text": "• ref_include_practice_2",
                  "fontSize": 14,
                  "fontColor": "#166534",
                  "bottomMargin": 8,
                  "lineHeight": 1.5
                },
                {
                  "type": "Label",
                  "text": "• ref_include_practice_3",
                  "fontSize": 14,
                  "fontColor": "#166534",
                  "bottomMargin": 8,
                  "lineHeight": 1.5
                },
                {
                  "type": "Label",
                  "text": "• ref_include_practice_4",
                  "fontSize": 14,
                  "fontColor": "#166534",
                  "bottomMargin": 8,
                  "lineHeight": 1.5
                },
                {
                  "type": "Label",
                  "text": "• ref_include_practice_5",
                  "fontSize": 14,
                  "fontColor": "#166534",
                  "lineHeight": 1.5
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}
