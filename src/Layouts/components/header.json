{
  "type": "View",
  "id": "header",
  "data": [
    {"class": "HeaderViewModel", "name": "viewModel"}
  ],
  "width": "matchParent",
  "height": 64,
  "background": "#FFFFFF",
  "orientation": "horizontal",
  "gravity": "centerVertical",
  "padding": [0, 24],
  "borderWidth": 1,
  "borderColor": "#E5E7EB",
  "child": [
    {
      "type": "View",
      "orientation": "horizontal",
      "gravity": "centerVertical",
      "child": [
        {
          "type": "Label",
          "id": "logo",
          "text": "JsonUI",
          "fontSize": 24,
          "fontWeight": "bold",
          "fontColor": "#087EA4",
          "onClick": "viewModel.onClickHome"
        }
      ]
    },
    {
      "type": "View",
      "weight": 1
    },
    {
      "type": "View",
      "orientation": "horizontal",
      "gravity": "centerVertical",
      "child": [
        {
          "type": "Label",
          "style": "nav_link",
          "text": "nav_learn",
          "margins": [0, 16, 0, 0],
          "onClick": "viewModel.onClickLearn"
        },
        {
          "type": "Label",
          "style": "nav_link",
          "text": "nav_reference",
          "margins": [0, 16, 0, 0],
          "onClick": "viewModel.onClickReference"
        },
        {
          "type": "Label",
          "style": "nav_link",
          "text": "nav_platforms",
          "margins": [0, 16, 0, 0],
          "onClick": "viewModel.onClickPlatforms"
        },
        {
          "type": "Label",
          "style": "nav_link",
          "text": "nav_community",
          "margins": [0, 16, 0, 0],
          "onClick": "viewModel.onClickCommunity"
        },
        {
          "type": "Label",
          "text": "GitHub",
          "fontColor": "#23272F",
          "fontSize": 16,
          "fontWeight": "medium",
          "margins": [0, 24, 0, 0],
          "onClick": "viewModel.onClickGitHub"
        }
      ]
    }
  ]
}
