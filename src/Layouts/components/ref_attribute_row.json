{
  "type": "View",
  "id": "ref_attribute_row_container",
  "orientation": "vertical",
  "width": "matchParent",
  "child": [
    { "data": [
      { "name": "name", "class": "String" },
      { "name": "uikitStatus", "class": "String" },
      { "name": "swiftuiStatus", "class": "String" },
      { "name": "composeStatus", "class": "String" },
      { "name": "xmlStatus", "class": "String" },
      { "name": "reactStatus", "class": "String" },
      { "name": "type", "class": "String" },
      { "name": "description", "class": "String" },
      { "name": "dividerVisibility", "class": "String" }
    ] },
    {
      "type": "View",
      "orientation": "horizontal",
      "width": "matchParent",
      "padding": [0, 16],
      "child": [
        {
          "type": "Label",
          "text": "@{name}",
          "fontSize": 13,
          "fontColor": "#1F2937",
          "font": "monospace",
          "weight": 2,
          "padding": [12]
        },
        {
          "type": "View",
          "width": 1,
          "height": "matchParent",
          "background": "#E5E7EB"
        },
        {
          "type": "Label",
          "text": "@{uikitStatus}",
          "fontSize": 14,
          "weight": 1,
          "textAlign": "Center",
          "padding": [12]
        },
        {
          "type": "View",
          "width": 1,
          "height": "matchParent",
          "background": "#E5E7EB"
        },
        {
          "type": "Label",
          "text": "@{swiftuiStatus}",
          "fontSize": 14,
          "weight": 1,
          "textAlign": "Center",
          "padding": [12]
        },
        {
          "type": "View",
          "width": 1,
          "height": "matchParent",
          "background": "#E5E7EB"
        },
        {
          "type": "Label",
          "text": "@{composeStatus}",
          "fontSize": 14,
          "weight": 1,
          "textAlign": "Center",
          "padding": [12]
        },
        {
          "type": "View",
          "width": 1,
          "height": "matchParent",
          "background": "#E5E7EB"
        },
        {
          "type": "Label",
          "text": "@{xmlStatus}",
          "fontSize": 14,
          "weight": 1,
          "textAlign": "Center",
          "padding": [12]
        },
        {
          "type": "View",
          "width": 1,
          "height": "matchParent",
          "background": "#E5E7EB"
        },
        {
          "type": "Label",
          "text": "@{reactStatus}",
          "fontSize": 14,
          "weight": 1,
          "textAlign": "Center",
          "padding": [12]
        },
        {
          "type": "View",
          "width": 1,
          "height": "matchParent",
          "background": "#E5E7EB"
        },
        {
          "type": "Label",
          "text": "@{type}",
          "fontSize": 13,
          "fontColor": "#6B7280",
          "weight": 2,
          "padding": [12]
        },
        {
          "type": "View",
          "width": 1,
          "height": "matchParent",
          "background": "#E5E7EB"
        },
        {
          "type": "Label",
          "text": "@{description}",
          "fontSize": 13,
          "fontColor": "#4B5563",
          "weight": 4,
          "padding": [12]
        }
      ]
    },
    {
      "type": "View",
      "width": "matchParent",
      "height": 1,
      "background": "#E5E7EB",
      "visibility": "@{dividerVisibility}"
    }
  ]
}
