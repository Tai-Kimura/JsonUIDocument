{
  "type": "View",
  "id": "ref_attribute_row_container",
  "orientation": "vertical",
  "width": "matchParent",
  "child": [
    { "data": [{ "class": "RefAttributeData", "name": "data" }] },
    {
      "type": "View",
      "orientation": "horizontal",
      "width": "matchParent",
      "padding": [0, 16],
      "child": [
        {
          "type": "Label",
          "text": "@{data.name}",
          "fontSize": 13,
          "fontColor": "#1F2937",
          "font": "monospace",
          "weight": 2,
          "padding": [12]
        },
        {
          "type": "View",
          "width": 1,
          "height": "matchParent",
          "background": "#E5E7EB"
        },
        {
          "type": "Label",
          "text": "@{data.uikit ? '✅' : '❌'}",
          "fontSize": 14,
          "weight": 1,
          "textAlign": "Center",
          "padding": [12]
        },
        {
          "type": "View",
          "width": 1,
          "height": "matchParent",
          "background": "#E5E7EB"
        },
        {
          "type": "Label",
          "text": "@{data.swiftui ? '✅' : '❌'}",
          "fontSize": 14,
          "weight": 1,
          "textAlign": "Center",
          "padding": [12]
        },
        {
          "type": "View",
          "width": 1,
          "height": "matchParent",
          "background": "#E5E7EB"
        },
        {
          "type": "Label",
          "text": "@{data.compose ? '✅' : '❌'}",
          "fontSize": 14,
          "weight": 1,
          "textAlign": "Center",
          "padding": [12]
        },
        {
          "type": "View",
          "width": 1,
          "height": "matchParent",
          "background": "#E5E7EB"
        },
        {
          "type": "Label",
          "text": "@{data.xml ? '✅' : '❌'}",
          "fontSize": 14,
          "weight": 1,
          "textAlign": "Center",
          "padding": [12]
        },
        {
          "type": "View",
          "width": 1,
          "height": "matchParent",
          "background": "#E5E7EB"
        },
        {
          "type": "Label",
          "text": "@{data.react ? '✅' : '❌'}",
          "fontSize": 14,
          "weight": 1,
          "textAlign": "Center",
          "padding": [12]
        },
        {
          "type": "View",
          "width": 1,
          "height": "matchParent",
          "background": "#E5E7EB"
        },
        {
          "type": "Label",
          "text": "@{data.type}",
          "fontSize": 13,
          "fontColor": "#6B7280",
          "weight": 2,
          "padding": [12]
        },
        {
          "type": "View",
          "width": 1,
          "height": "matchParent",
          "background": "#E5E7EB"
        },
        {
          "type": "Label",
          "text": "@{data.description}",
          "fontSize": 13,
          "fontColor": "#4B5563",
          "weight": 4,
          "padding": [12]
        }
      ]
    },
    {
      "type": "View",
      "width": "matchParent",
      "height": 1,
      "background": "#E5E7EB",
      "visibility": "@{data.isLast ? 'gone' : 'visible'}"
    }
  ]
}
